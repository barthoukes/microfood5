<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/popup_not_implemented_layout"
    android:padding="20dip"
    android:background="@color/ANDROID_MESSAGE_BACKGROUND"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
	<TableLayout
		android:layout_width="fill_parent" 
		android:layout_height="wrap_content">
		<TableRow 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content">
	    	<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="20sp"
		        android:textColor="#ffFFFFFF"
		        android:text="Print hoevaak ?"
		        android:textSize="25sp"
		        android:padding="20dip"
		        android:id="@+id/text_hoevaak"/>
	    </TableRow>
	    <TableRow 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content">
		    <Button
			    android:id="@+id/cancel_ok"
			    android:layout_width="0dp"
			    android:minWidth="25dp"
			    android:layout_height="wrap_content"
			    android:background="@drawable/cancel"
			    android:gravity="center"
			    android:layout_weight="1"
			    android:layout_margin="12dp"/>
			<Button
			    android:id="@+id/print_0x"
			    android:layout_height="wrap_content"
			    android:background="#a0a0FF"
			    android:layout_weight="1"
			    android:padding="25dip"
 				android:layout_margin="12dp"
			    android:text="0x"
			    android:textColor="#FFFFFFFF"
			    android:textSize="25sp" />
			<Button
			    android:id="@+id/print_1x"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:background="#a0a0FF"
			    android:padding="25dip"
			    android:text="1x"
			    android:layout_margin="12dp"
			    android:textColor="#FFFFFFFF"
			    android:textSize="25sp" />
			<Button
			    android:id="@+id/print_2x"
			    android:layout_weight="1"
			    android:layout_height="wrap_content"
			    android:background="#a0a0FF"
			    android:padding="25dip"
			    android:text="2x"
			    android:layout_margin="12dp"
			    android:textColor="#FFFFFFFF"
			    android:textSize="25sp" />
		</TableRow>
	</TableLayout>
</LinearLayout>
